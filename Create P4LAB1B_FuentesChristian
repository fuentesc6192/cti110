# This Code draws letters


import turtle


screen = turtle.Screen()
t = turtle.Turtle()


t.color("purple")  
t.pensize(5)       

t.penup()
t.goto(-100, 50)   
t.pendown()
t.right(90)       
t.forward(100)     
t.circle(-30, 180) 

t.penup()
t.goto(50, 50)     
t.pendown()

t.left(90)         
t.forward(100)    
t.right(90)       
t.circle(-30, 180)

turtle.done()
input()
